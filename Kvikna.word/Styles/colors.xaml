<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Kvikna.word.Styles">

    <Color x:Key="BackgroundLight">#efefef</Color>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}" />

    <Color x:Key="BackgroundVeryLight">#000fef</Color>
    <SolidColorBrush x:Key="BackgroundVeryLightBrush" Color="{StaticResource BackgroundVeryLight}" />

    <Color x:Key="ForgroundMain">#89567545</Color>
    <SolidColorBrush x:Key="ForgroundMainBrush" Color="{StaticResource ForgroundMain}" />

    <Color x:Key="BackgroundVeryDark">#00000</Color>
    <SolidColorBrush x:Key="BackgroundVeryDarkBrush" Color="{StaticResource BackgroundVeryDark}" />

    <Color x:Key="ForgroundLight">#ffffff</Color>
    <SolidColorBrush x:Key="ForgroundLightBrush" Color="{StaticResource ForgroundLight}" />


</ResourceDictionary>
    